<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Value: the combination of each digit, Support(1)/Unsupport(0)
            - 1st: Emergency Number over IMS Emergency bearer
            - 2nd: Emergency Number over IMS Nomral bearer
            - 3rd: Emergency Number over CS
        - Category: the service category
        - Condition: there are following values for emergency routing
                     except CONDITION_UI_ONLY
            CONDITION_ALWAYS                0x00001
            CONDITION_NO_SIM                0x00010
            CONDITION_TESTCARD              0x00100
            CONDITION_NOT_IMS_REGI          0x01000
            CONDITION_UI_ONLY               0x10000
    -->
    <MccmncValue Mccmnc="710300">
        <EccEntry Number="112" Category="0" Condition="0x00001" />
        <EccEntry Number="911" Category="0" Condition="0x00001" />
        <EccEntry Number="118" Category="0" Condition="0x00001" />
        <EccEntry Number="122" Category="0" Condition="0x00001" />
        <EccEntry Number="115" Category="0" Condition="0x00001" />
    </MccmncValue>
    <MccmncValue Mccmnc="#R">
        <EccEntry Number="112" Category="0" Condition="0x00001" />
        <EccEntry Number="911" Category="0" Condition="0x00001" />
        <EccEntry Number="115" Category="0" Condition="0x00001" />
        <EccEntry Number="118" Category="0" Condition="0x00001" />
        <EccEntry Number="128" Category="0" Condition="0x00001" />
    </MccmncValue>
</EccTable>
