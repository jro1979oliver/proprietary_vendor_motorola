<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Value: the combination of each digit, Support(1)/Unsupport(0)
            - 1st: Emergency Number over IMS Emergency bearer
            - 2nd: Emergency Number over IMS Nomral bearer
            - 3rd: Emergency Number over CS
        - Category: the service category
        - Condition: there are following values for emergency routing
                     except CONDITION_UI_ONLY
            CONDITION_ALWAYS                0x00001
            CONDITION_NO_SIM                0x00010
            CONDITION_TESTCARD              0x00100
            CONDITION_NOT_IMS_REGI          0x01000
            CONDITION_UI_ONLY               0x10000
    -->
    <MccmncValue Mccmnc="#R">
        <EccEntry Number="010" Category="0" Condition="0x10000" />
        <EccEntry Number="020" Category="0" Condition="0x10000" />
        <EccEntry Number="030" Category="0" Condition="0x10000" />
        <EccEntry Number="040" Category="0" Condition="0x10000" />
        <EccEntry Number="101" Category="0" Condition="0x10000" />
        <EccEntry Number="102" Category="0" Condition="0x10000" />
        <EccEntry Number="103" Category="0" Condition="0x10000" />
        <EccEntry Number="104" Category="0" Condition="0x10000" />
        <EccEntry Number="112" Category="0" Condition="0x10000" />
        <EccEntry Number="901" Category="0" Condition="0x10000" />
        <EccEntry Number="902" Category="0" Condition="0x10000" />
        <EccEntry Number="903" Category="0" Condition="0x10000" />
        <EccEntry Number="904" Category="0" Condition="0x10000" />
    </MccmncValue>
</EccTable>
